{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "CustomLoggers":{
      "SplunkLogger": {
        "Protocol": "http",
        "Host": "localhost",
        "Port": "3000",
        "Username": "Username",
        "Password": "Password",
        "SourceType": "SourceType",
        "Index": "Index"
      },
      "MattermostLogger": {
        "Protocol": "http",
        "Host": "localhost",
        "Port": "3000",
        "Path": "bla/6"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails"
    ]
  },
  "Service": {
    "Name": "bp-seed",
    "Version": "0.0.0"
  },
  "AllowedHosts": "*",
  "Kestrel":{
    "EndPoints":{
      "Http":{
        "Url": "http://*:5000"
      }
    }
  }
}
